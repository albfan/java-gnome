(define-enum ButtonsType
  (in-module "Gtk")
  (c-name "GtkButtonsType")
  (values
    '("none" "GTK_BUTTONS_NONE")
    '("ok" "GTK_BUTTONS_OK")
    '("close" "GTK_BUTTONS_CLOSE")
    '("cancel" "GTK_BUTTONS_CANCEL")
    '("yes_no" "GTK_BUTTONS_YES_NO")
    '("ok_cancel" "GTK_BUTTONS_OK_CANCEL")
  )
)

