(define-object DOMStyleMedia
  (in-module "WebKit")
  (import-header "webkit/webkit.h")
  (parent "DOMObject")
  (c-name "WebKitDOMStyleMedia")
)

(define-method match_medium
  (of-object "WebKitDOMStyleMedia")
  (c-name "webkit_dom_style_media_match_medium")
  (return-type "gboolean")
  (parameters
    '("const gchar*" "pmediaquery")
  )
)

