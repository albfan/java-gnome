    '("widget" "ATK_LAYER_WIDGET")
    '("mdi" "ATK_LAYER_MDI")
    '("popup" "ATK_LAYER_POPUP")
    '("overlay" "ATK_LAYER_OVERLAY")
    '("window" "ATK_LAYER_WINDOW")
