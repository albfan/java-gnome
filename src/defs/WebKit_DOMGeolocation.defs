(define-object DOMGeolocation
  (in-module "WebKit")
  (import-header "webkit/webkit.h")
  (parent "DOMObject")
  (c-name "WebKitDOMGeolocation")
)

(define-method clear_watch
  (of-object "WebKitDOMGeolocation")
  (c-name "webkit_dom_geolocation_clear_watch")
  (return-type "none")
  (parameters
    '("glong" "pwatchID")
  )
)

