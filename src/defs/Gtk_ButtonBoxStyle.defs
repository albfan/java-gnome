  (values
    '("spread" "GTK_BUTTONBOX_SPREAD")
    '("edge" "GTK_BUTTONBOX_EDGE")
    '("start" "GTK_BUTTONBOX_START")
    '("end" "GTK_BUTTONBOX_END")
    '("center" "GTK_BUTTONBOX_CENTER")
    '("expand" "GTK_BUTTONBOX_EXPAND")
  )
)

