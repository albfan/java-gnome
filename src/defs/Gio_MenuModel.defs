  (c-name "get_item_attribute_value")
  (return-type "GVariant*")
  (parameters
    '("gint" "pitem_index")
    '("const gchar*" "pattribute")
    '("const GVariantType*" "pexpected_type")
  )
)

(define-method get_item_attributes
  (of-object "GMenuModel")
  (c-name "get_item_attributes")
  (return-type "none")
  (parameters
    '("gint" "pitem_index")
    '("GHashTable**" "pattributes")
  )
)

(define-method get_item_link
  (of-object "GMenuModel")
  (c-name "get_item_link")
  (return-type "GMenuModel*")
  (parameters
    '("gint" "pitem_index")
    '("const gchar*" "plink")
  )
)

(define-method get_item_links
  (of-object "GMenuModel")
  (c-name "get_item_links")
  (return-type "none")
  (parameters
    '("gint" "pitem_index")
    '("GHashTable**" "plinks")
  )
)

(define-method get_n_items
  (of-object "GMenuModel")
