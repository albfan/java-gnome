    '("gint" "pitem_index")
  )
)

(define-method get_item_attribute
  (of-object "GMenuModel")
  (c-name "g_menu_model_get_item_attribute")
  (return-type "gboolean")
  (parameters
    '("gint" "pitem_index")
    '("const gchar*" "pattribute")
    '("const gchar*" "pformat_string")
    '("null" "p...")
  )
)

