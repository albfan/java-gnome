  (parameters
  )
)

(define-method get_password
  (of-object "GProxyAddress")
  (c-name "g_proxy_address_get_password")
  (return-type "const gchar*")
  (parameters
  )
)

(define-method get_protocol
  (of-object "GProxyAddress")
  (c-name "g_proxy_address_get_protocol")
  (return-type "const gchar*")
  (parameters
  )
)

(define-method get_uri
  (of-object "GProxyAddress")
